{
  "name": "billsbear",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev:watch": "nodemon src/app.ts",
    "dev": "ts-node src/app.ts",
    "start": "node dist/src/app.js",
    "build": "tsc",
    "clean": "rimraf dist/*",
    "test": "jest --env=node test",
    "test::watch": "jest --env=node --watch test",
    "test::watch::unit": "jest --env=node --watch /tests/unit",
    "test::watch::integration": "jest --env=node --watch /tests/integration",
    "test::watch::e2e": "jest --env=node --watch /tests/e2e",
    "test::coverage": "jest --env=node --colors --coverage test",
    "lint": "eslint  . --ext .js,.jsx,.ts,.tsx --fix",
    "precommit": "eslint --cache --fix && jest --env=node --color --coverage test",
    "prepare": "husky install"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/jest": "^27.0.2",
    "@types/node": "^16.10.0",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.4",
    "jest": "^27.3.1",
    "lint-staged": "^11.2.6",
    "nodemon": "^2.0.13",
    "prettier": "^2.4.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.0.7",
    "ts-node": "^10.2.1",
    "typescript": "^4.4.4"
  },
  "dependencies": {
    "exceljs": "^4.3.0"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": "npm run precommit"
  }
}
